<div id="content">
    <h1>New Project</h1>
    
    <%= error_messages_for :hr_si_project %>
    
    <% form_for(:hr_si_project, :url => hr_si_projects_path) do |f| %>
        <%= render :partial => 'form', :locals => {:f => f}%>
        
        <p style="margin-top: 36px;"><%= submit_tag "Create" %> or <%= link_to "Cancel", hr_si_projects_path, :class => "cancel" %></p>
    <% end %>
</div>
