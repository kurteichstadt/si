<div id="content">
    <h1>New User</h1>

    <%= error_messages_for :temp_user %>
    
    <% form_for(:temp_user, :url => users_path, :html => {:id => "user_form"}) do |f| %>
        <%= render :partial => 'form', :locals => {:f => f}%>
        
        <p style="margin-top: 36px;"><%= submit_tag "Create" %> or <%= link_to "Cancel", users_path, :class => "cancel" %></p>
    <% end %>
</div>
